{
    "compilerOptions": {
      "target": "ES2018",
      "module": "commonjs",
      "lib": ["es2018"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": ".",
      "rootDir": ".",
      "sourceMap": true,
      "moduleResolution": "node"
    },
    "exclude": ["node_modules"]
  }